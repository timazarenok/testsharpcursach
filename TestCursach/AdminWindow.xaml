<Window x:Class="TestCursach.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestCursach"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="EditTest" Width="150" Height="30" Click="EditTest_Click" Margin="0,0,0,0">Редактирование теста</Button>
        <Button x:Name="Exit" Width="120" Height="25" Content="Выход" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,20,10" Click="Exit_Click"/>
    </Grid>
</Window>
